package com.example;

import java.io.File;
import java.io.IOException;

public class Java8Example {
	
public static void main(String[] args) {
	File f=new File("f1.txt");
	try {
		boolean b=f.createNewFile();
		f.delete();
		String path=f.getAbsolutePath();
		System.out.println(path);
	} catch (IOException e) {
		
		e.printStackTrace();
	}
	
	
	
}
}
